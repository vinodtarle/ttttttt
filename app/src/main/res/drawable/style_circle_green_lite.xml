<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <gradient android:angle="90" android:endColor="@color/colorYD" android:startColor="@color/colorYD" />
            <corners android:radius="10dp" />
            <stroke android:width="11dp" android:color="@color/colorYD" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <gradient android:angle="45" android:endColor="@color/colorYD" android:startColor="@color/colorYD" />
            <corners android:radius="10dp" />
            <stroke android:width="11dp" android:color="@color/colorYD" />
        </shape>
    </item>
</selector>