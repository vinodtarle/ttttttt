<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/moduleBorderMeeting" />
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="30dp" />
            <corners
                android:topLeftRadius="5dp"
                android:topRightRadius="5dp" />
            <gradient
                android:angle="90"
                android:centerColor="@color/moduleBorderMeeting"
                android:endColor="@color/moduleBorderMeeting"
                android:startColor="@color/moduleBorderMeeting" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
            <solid android:color="@color/white" />
        </shape>
    </item>
</layer-list>